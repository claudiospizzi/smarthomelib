{
  "name": "smarthomelib",
  "version": "0.0.0",
  "description": "TypeScript library to integrate various devices into a Loxone smart home.",
  "keywords": [
    "smarthome",
    "loxone",
    "mqtt",
    "influxdb"
  ],
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src --ext .ts"
  },
  "author": "Claudio Spizzi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claudiospizzi/smarthomelib.git"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "influx": "^5.8.0",
    "mqtt": "^4.2.6",
    "tslog": "^3.1.2"
  }
}
